        MENUITEM SEPARATOR
        MENUITEM "アプリケーションの終了(&X)\tCtrl+Alt+Shift+X", ID_APP_EXIT
    END
    POPUP "表示(&V)"
    BEGIN
        POPUP "ツールバー(&T)"
        BEGIN
            MENUITEM "標準(&T)",                    ID_VIEW_TOOLBAR
            MENUITEM "選択(&S)",                    ID_VIEW_TOOLBAR_SELECT
            MENUITEM "ウインドウ(&W)",              ID_VIEW_TOOLBAR_WINDOW
            MENUITEM SEPARATOR
            MENUITEM "ツールバーを固定(&F)",        ID_VIEW_TOOLBAR_FIX
        END
        MENUITEM "ステータスバー(&S)",          ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        MENUITEM "常に手前に表示(&A)",          ID_VIEW_TOPMOST
    END
    POPUP "設定(&S)"
    BEGIN
        MENUITEM "環境設定(&E)...\tAlt+Enter",  ID_SETTING_ENVIRONMENT
        MENUITEM SEPARATOR
    END
    POPUP "ヘルプ(&H)"
    BEGIN
        MENUITEM "目次(&C)\tF1",                ID_HELP
        MENUITEM "キーワード(&K)",              ID_HELP_INDEX
