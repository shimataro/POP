IDR_MAINFRAME MENU 
BEGIN
    POPUP "ファイル(&F)"
    BEGIN
        MENUITEM "新規作成(&N)...\tCtrl+N",     ID_FILE_NEW
        MENUITEM "開く(&O)...\tCtrl+O",         ID_FILE_OPEN
        MENUITEM SEPARATOR
        POPUP "取り込み(&I)"
        BEGIN
            MENUITEM "クリップボードから(&C)",      ID_FILE_IMPORT_CLIPBOARD
            MENUITEM "画面から(&D)...",             ID_FILE_IMPORT_DISPLAY
        END
        MENUITEM SEPARATOR
